#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@graphql-codegen+cli@5.0.5_@types+node@22.13.5_graphql@16.10.0_typescript@5.7.3/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@graphql-codegen+cli@5.0.5_@types+node@22.13.5_graphql@16.10.0_typescript@5.7.3/node_modules/@graphql-codegen/cli/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@graphql-codegen+cli@5.0.5_@types+node@22.13.5_graphql@16.10.0_typescript@5.7.3/node_modules/@graphql-codegen/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@graphql-codegen+cli@5.0.5_@types+node@22.13.5_graphql@16.10.0_typescript@5.7.3/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@graphql-codegen+cli@5.0.5_@types+node@22.13.5_graphql@16.10.0_typescript@5.7.3/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@graphql-codegen+cli@5.0.5_@types+node@22.13.5_graphql@16.10.0_typescript@5.7.3/node_modules/@graphql-codegen/cli/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@graphql-codegen+cli@5.0.5_@types+node@22.13.5_graphql@16.10.0_typescript@5.7.3/node_modules/@graphql-codegen/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@graphql-codegen+cli@5.0.5_@types+node@22.13.5_graphql@16.10.0_typescript@5.7.3/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
fi
