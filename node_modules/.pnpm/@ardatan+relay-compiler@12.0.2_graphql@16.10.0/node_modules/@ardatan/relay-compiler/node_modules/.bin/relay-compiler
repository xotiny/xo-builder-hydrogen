#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@ardatan+relay-compiler@12.0.2_graphql@16.10.0/node_modules/@ardatan/relay-compiler/bin/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@ardatan+relay-compiler@12.0.2_graphql@16.10.0/node_modules/@ardatan/relay-compiler/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@ardatan+relay-compiler@12.0.2_graphql@16.10.0/node_modules/@ardatan/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@ardatan+relay-compiler@12.0.2_graphql@16.10.0/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@ardatan+relay-compiler@12.0.2_graphql@16.10.0/node_modules/@ardatan/relay-compiler/bin/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@ardatan+relay-compiler@12.0.2_graphql@16.10.0/node_modules/@ardatan/relay-compiler/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@ardatan+relay-compiler@12.0.2_graphql@16.10.0/node_modules/@ardatan/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/@ardatan+relay-compiler@12.0.2_graphql@16.10.0/node_modules:/Users/vandoan/Documents/code/hydrogen/xo-builder-hydrogen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/relay-compiler" "$@"
else
  exec node  "$basedir/../../bin/relay-compiler" "$@"
fi
